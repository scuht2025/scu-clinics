{
  "name": "scu-clinics",
  "productName": "scu-clinics",
  "version": "1.5.0",
  "description": "Clinics Manger",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "start:dev": "NODE_ENV=development electron-forge start",
    "start:prod": "NODE_ENV=production electron-forge start",
    "package": "electron-forge package",
    "package:win": "electron-forge make --platform=win32",
    "package:mac": "electron-forge make --platform=darwin",
    "package:linux": "electron-forge make --platform=linux",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "release:patch": "node scripts/release.js patch && git push origin main --tags",
    "release:minor": "node scripts/release.js minor && git push origin main --tags",
    "release:major": "node scripts/release.js major && git push origin main --tags",
    "build:ico": "node scripts/build-ico.js",
    "premake": "npm run build:ico",
    "prepackage:win": "npm run build:ico",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "type-check": "tsc --noEmit",
    "build": "npm run type-check && npm run lint",
    "clean": "rimraf dist out .vite",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "keywords": [],
  "author": {
    "name": "scuht2025",
    "email": "scuht2025@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/publisher-github": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^20.19.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "38.1.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "png-to-ico": "^2.1.8",
    "rimraf": "^5.0.5",
    "typescript": "~4.5.4",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "fuse.js": "^6.6.2",
    "sqlite3": "^5.1.7",
    "quill": "^1.3.7",
    "quill-mention": "^3.1.0",
    "@types/quill": "^1.3.10"
  }
}
